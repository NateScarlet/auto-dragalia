{
  "name": "auto-dragalia",
  "version": "0.12.0",
  "private": true,
  "main": "src/main.ts",
  "repository": "https://github.com/NateScarlet/auto-dragalia.git",
  "author": "NateScarlet <NateScarlet@Gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "watch": "webpack --watch",
    "capture": "autojs cap",
    "code-generate:images": "node ./scripts/generate-images-code.js",
    "adb:push": "node ./scripts/push.js",
    "release": "standard-version"
  },
  "dependencies": {
    "lodash.sortby": "4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.6.0",
    "@commitlint/config-conventional": "7.6.0",
    "@types/lodash.sortby": "4.7.6",
    "autojs-dev": "0.7.1",
    "cross-env": "5.2.0",
    "husky": "2.2.0",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "raw-loader": "2.0.0",
    "standard-version": "6.0.1",
    "ts-loader": "6.0.0",
    "tslint": "5.16.0",
    "tslint-microsoft-contrib": "6.1.1",
    "typescript": "3.4.5",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{md,ts,js,yml,xml,json}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "node ./scripts/postchangelog.js"
    }
  }
}
