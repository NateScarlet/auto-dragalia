{
  "name": "auto-dragalia",
  "version": "0.13.1",
  "private": true,
  "main": "src/main.ts",
  "repository": "https://github.com/NateScarlet/auto-dragalia.git",
  "author": "NateScarlet <NateScarlet@Gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "node ./scripts/build.js",
    "watch": "webpack --watch",
    "capture": "autojs cap",
    "lint": "eslint --ext .js,.ts src",
    "code-generate:images": "node ./scripts/generate-images-code.js",
    "code-generate:locale": "node ./scripts/generate-locale-schema.js",
    "adb:push": "node ./scripts/push.js",
    "release": "standard-version"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "ajv": "6.12.3",
    "autojs-dev": "0.7.1",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "raw-loader": "4.0.1",
    "standard-version": "8.0.1",
    "ts-loader": "6.2.2",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{md,ts,js,yml,xml,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "node ./scripts/postchangelog.js"
    }
  }
}
